{
  "name": "hamter",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "node --no-warnings=ExperimentalWarning ./scripts/clear-dev-migration-marker.mjs && cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env PAYLOAD_PUSH_SCHEMA=true NODE_OPTIONS=--no-deprecation next dev -p 3052",
    "check": "biome check",
    "check:fix": "biome check --fix --unsafe",
    "start": "node --no-warnings=ExperimentalWarning ./scripts/clear-dev-migration-marker.mjs && cross-env NODE_OPTIONS=--no-deprecation next start -p 3052",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "backup:data": "sh ./scripts/backup-data.sh",
    "restore:data": "sh ./scripts/restore-data.sh"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@payloadcms/db-sqlite": "^3.76.1",
    "@payloadcms/live-preview-react": "^3.76.1",
    "@payloadcms/next": "^3.76.1",
    "@payloadcms/richtext-lexical": "^3.76.1",
    "@payloadcms/ui": "^3.76.1",
    "@radix-ui/react-progress": "^1.1.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@use-gesture/react": "^10.3.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.34.0",
    "geist": "^1.7.0",
    "graphql": "^16.12.0",
    "lodash": "^4.17.23",
    "lucide-react": "^0.454.0",
    "next": "15.4.11",
    "payload": "^3.76.1",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.4",
    "rss": "^1.2.2",
    "sharp": "0.34.2",
    "sugar-high": "^0.9.5",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/canvas-confetti": "^1.9.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/rss": "^0.0.32"
  }
}
